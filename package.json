{
  "name": "redgoose.me",
  "version": "5.7.0",
  "repository": "https://github.com/redgoose-dev/redgoose.me.git",
  "author": "redgoose <scripter@me.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node --no-warnings server/entry.js --development",
    "dev:live": "nodemon --no-warnings server/entry.js --development",
    "prebuild": "rm -rf ./dist",
    "build": "vite build",
    "start": "node --no-warnings server/entry.js --production",
    "start:live": "nodemon --no-warnings server/entry.js --production"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": ["src/**/*", "dist/**/*"]
  },
  "engines": {
    "node": "20"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "isbot": "^3.6.10",
    "marked": "4.3.0",
    "ohmyfetch": "^0.4.21",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@redgoose/markdown-style": "^1.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@tsconfig/svelte": "^4.0.1",
    "@types/express": "^4.17.17",
    "@types/marked": "^4.3.0",
    "@types/node": "^18.16.3",
    "auto-writer": "^1.1.2",
    "esbuild": "^0.17.18",
    "nodemon": "^2.0.22",
    "sass": "^1.62.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tinro": "^0.6.12",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4"
  }
}
