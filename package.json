{
  "name": "redgoose.me",
  "version": "5.6.0",
  "repository": "https://gitlab.com/redgoose-me/redgoose.me.git",
  "author": "redgoose <scripter@me.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node --no-warnings server/entry.js --development",
    "dev:live": "nodemon --no-warnings server/entry.js --development",
    "prebuild": "rm -rf ./dist",
    "build": "vite build",
    "start": "node server/entry.js --production"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": ["src/**/*", "dist/**/*"]
  },
  "engines": {
    "node": "16"
  },
  "browserslist": ["> 1%", "last 2 versions"],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "isbot": "^3.6.0",
    "marked": "^4.1.1",
    "ohmyfetch": "^0.4.19",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@redgoose/markdown-style": "^1.1.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "@tsconfig/svelte": "^3.0.0",
    "@types/express": "^4.17.14",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.7.23",
    "auto-writer": "^1.1.2",
    "nodemon": "^2.0.20",
    "sass": "^1.55.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tinro": "^0.6.12",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  }
}