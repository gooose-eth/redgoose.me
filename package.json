{
  "name": "redgoose.me",
  "version": "5.6.7",
  "repository": "https://github.com/redgoose-dev/redgoose.me.git",
  "author": "redgoose <scripter@me.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node --no-warnings server/entry.js --development",
    "dev:live": "nodemon --no-warnings server/entry.js --development",
    "prebuild": "rm -rf ./dist",
    "build": "vite build",
    "start": "node --no-warnings server/entry.js --production",
    "start:live": "nodemon --no-warnings server/entry.js --production"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": ["src/**/*", "dist/**/*"]
  },
  "engines": {
    "node": "16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "marked": "^4.2.12",
    "ohmyfetch": "^0.4.21",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@redgoose/markdown-style": "^1.1.4",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.18",
    "auto-writer": "^1.1.2",
    "nodemon": "^2.0.20",
    "sass": "^1.58.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "tinro": "^0.6.12",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  }
}
