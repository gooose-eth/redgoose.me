{
  "name": "redgoose.me",
  "version": "5.6.6",
  "repository": "https://github.com/redgoose-dev/redgoose.me.git",
  "author": "redgoose <scripter@me.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node --no-warnings server/entry.js --development",
    "dev:live": "nodemon --no-warnings server/entry.js --development",
    "prebuild": "rm -rf ./dist",
    "build": "vite build",
    "start": "node --no-warnings server/entry.js --production",
    "start:live": "nodemon --no-warnings server/entry.js --production"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ignore": ["src/**/*", "dist/**/*"]
  },
  "engines": {
    "node": "16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "marked": "^4.2.2",
    "ohmyfetch": "^0.4.21",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@redgoose/markdown-style": "^1.1.4",
    "@sveltejs/vite-plugin-svelte": "^1.1.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/express": "^4.17.14",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.9",
    "auto-writer": "^1.1.2",
    "nodemon": "^2.0.20",
    "sass": "^1.56.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tinro": "^0.6.12",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  }
}
