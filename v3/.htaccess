RewriteEngine On
RewriteCond $1 !\.(js|ico|gif|jpg|jpeg|png|css|swf|mp3|wav|txt|zip|rar|htc|pdf|scss|sass)$
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1

